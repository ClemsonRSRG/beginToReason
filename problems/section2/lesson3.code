Facility BeginToReason;
    uses Integer_Ext_Theory;

    Operation Main();
    Procedure
        Var I, J: Integer;
        Read(I);
        Read(J);

        Remember;

        I := I + J;
        Confirm I = /*expression*/;

        J := I - J;
        Confirm J = /*expression*/;

        I := I - J;
        Confirm I = /*expression*/;
        Confirm J = /*expression*/;
    end Main;
end BeginToReason;
